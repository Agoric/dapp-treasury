{
  "name": "@agoric/dapp-treasury-ui",
  "version": "0.0.1",
  "description": "Treasury Frontend",
  "author": "Agoric",
  "license": "Apache-2.0",
  "homepage": ".",
  "scripts": {
    "clean": "rimraf ./build",
    "lint-check": "yarn lint",
    "lint": "yarn lint:eslint",
    "lint:eslint": "eslint '**/*.{js,jsx}'",
    "lint-fix": "eslint --fix '**/*.{js,jsx}'",
    "start": "yarn build:ses && react-scripts start",
    "test": "TZ=UTC react-scripts test",
    "test:debug": "TZ=UTC react-scripts --inspect-brk test --runInBand --no-cache",
    "build": "yarn build:ses && yarn build:react",
    "build:ses": "cp ../node_modules/ses/dist/lockdown.umd.js public/",
    "build:react": "react-scripts build",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@endo/captp": "^1.10.12",
    "@agoric/ertp": "dev",
    "@endo/eventual-send": "^0.14.4",
    "@endo/nat": "^4.1.4",
    "@agoric/notifier": "dev",
    "@agoric/same-structure": "dev",
    "@agoric/ui-components": "dev",
    "@agoric/wallet-connection": "dev",
    "@agoric/zoe": "dev",
    "@agoric/run-protocol": "dev",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/styles": "^4.11.3",
    "autodux": "^5.0.3",
    "clsx": "^1.0.4",
    "ethers": "^5.0.25",
    "http-proxy-middleware": "^1.0.0",
    "json5": "^2.1.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-number-format": "^4.4.3",
    "react-router-dom": "^5.2.0",
    "ses": "^0.15.7"
  },
  "devDependencies": {
    "@babel/eslint-plugin": "^7.12.1",
    "@endo/eslint-plugin": "^0.3.20",
    "@jessie.js/eslint-plugin": "^0.1.3",
    "babel-eslint": "^10.1.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4",
    "prettier": "^1.19.1",
    "react-scripts": "^4.0.3",
    "rimraf": "^3.0.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint-plugin-jest": "^24.5.2"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!@lit-labs)/"
    ]
  },
  "eslintConfig": {
    "ignorePatterns": [
      "**/*.umd.js",
      "**/generated/*"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "jest"
    ],
    "rules": {
      "react/prop-types": "off",
      "react/react-in-jsx-scope": "off"
    },
    "env": {
      "browser": true,
      "jest/globals": true
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      },
      "react": {
        "version": "detect"
      }
    },
    "extends": [
      "@agoric",
      "plugin:react/recommended"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  }
}
