{
  "name": "@agoric/dapp-autoswap-ui",
  "version": "0.0.1",
  "description": "Autoswap Frontend",
  "author": "Agoric",
  "license": "Apache-2.0",
  "homepage": ".",
  "scripts": {
    "clean": "rimraf ./build",
    "lint-check": "eslint '**/*.{js,jsx}'",
    "lint-fix": "eslint --fix '**/*.{js,jsx}'",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "build": "react-scripts build",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.9.3",
    "@material-ui/icons": "^4.9.1",
    "autodux": "^5.0.3",
    "clsx": "^1.0.4",
    "http-proxy-middleware": "^1.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/eslint-plugin": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "prettier": "^1.19.1",
    "react-scripts": "^4.0.1",
    "rimraf": "^3.0.2"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "env": {
      "browser": true
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      },
      "react": {
        "version": "detect"
      }
    },
    "extends": [
      "@agoric",
      "plugin:react/recommended"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  }
}
