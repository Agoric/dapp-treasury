{
  "name": "@agoric/dapp-treasury-ui",
  "version": "0.0.1",
  "description": "Treasury Frontend",
  "author": "Agoric",
  "license": "Apache-2.0",
  "homepage": ".",
  "scripts": {
    "clean": "rimraf ./build",
    "lint-check": "yarn lint",
    "lint": "yarn lint:eslint",
    "lint:eslint": "eslint '**/*.{js,jsx}'",
    "lint-fix": "eslint --fix '**/*.{js,jsx}'",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "build": "yarn build:react",
    "build:ses": "cp ../node_modules/ses/dist/lockdown.umd.js public/",
    "build:react": "yarn configure; yarn build:pre-patch; react-scripts build; yarn build:post-patch",
    "build:pre-patch": "node -r esm ./ses-patch.js src/install-ses-lockdown.js",
    "build:post-patch": "node -r esm ./ses-patch.js build/index.html",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@agoric/assert": "*",
    "@endo/captp": "^1.10.12",
    "@agoric/ertp": "*",
    "@agoric/eventual-send": "*",
    "@agoric/install-ses": "*",
    "@agoric/nat": "^4.0.0",
    "@agoric/notifier": "*",
    "@agoric/same-structure": "*",
    "@agoric/ui-components": "*",
    "@agoric/wallet-connection": "*",
    "@agoric/zoe": "*",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/styles": "^4.11.3",
    "autodux": "^5.0.3",
    "clsx": "^1.0.4",
    "ethers": "^5.0.25",
    "http-proxy-middleware": "^1.0.0",
    "json5": "^2.1.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-number-format": "^4.4.3",
    "react-router-dom": "^5.2.0",
    "ses": "^0.12.7"
  },
  "devDependencies": {
    "@babel/eslint-plugin": "^7.12.1",
    "@endo/eslint-plugin": "^0.3.10",
    "@jessie.js/eslint-plugin": "^0.1.3",
    "babel-eslint": "^10.1.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4",
    "prettier": "^1.19.1",
    "react-scripts": "^4.0.3",
    "rimraf": "^3.0.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint-plugin-jest": "^24.5.2"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!@lit-labs)/"
    ]
  },
  "eslintConfig": {
    "ignorePatterns": [
      "**/*.umd.js",
      "**/generated/*"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "jest"
    ],
    "rules": {
      "react/prop-types": "off",
      "react/react-in-jsx-scope": "off"
    },
    "env": {
      "browser": true,
      "jest/globals": true
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      },
      "react": {
        "version": "detect"
      }
    },
    "extends": [
      "@agoric",
      "plugin:react/recommended"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  }
}
